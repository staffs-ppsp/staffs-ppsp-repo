%DEFAULT_PROPERTIES%
template-js-external
empty
template-error-display
nodisplay
%MAIN_CONTENT%
<html>
	<head>
		<title>K.C.Hilton Cinema Chain - Booking Manager</title>
		<link rel="stylesheet" type="text/css" href="/resources/css/theme" media="all" />
		<link rel="stylesheet" type="text/css" href="/resources/css/page" media="all" />
		<link rel="stylesheet" type="text/css" href="/resources/css/print" media="print" />
		<meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
		<script language="JavaScript" type="text/javascript" src="/resources/js/prototype"></script>
		<script language="JavaScript" type="text/javascript" src="/resources/js/general"></script>
		<script language="JavaScript" type="text/javascript" src="/resources/js/${template-js-external}"></script>
		<script language="JavaScript" type="text/javascript">
			${template-js-inline}
		</script>
	</head>

	<body>

		<table border="0" cellpadding="0" cellspacing="0" width="100%" class="page-table">
			<tr>
				<td align="left" valign="middle" class="header page-header noprint" colspan="2">
					K.C.Hilton Cinema Chain - Booking Manager
				</td>
			</tr>
			<tr>
				<td align="left" valign="middle" class="toolbar page-toolbar noprint" colspan="2">
					<a href="/book" class="${template-toolbar-book}">Make A Booking</a>
				</td>
			</tr>
			<tr>
				<td align="left" valign="middle" width="250" class="container noprint">
					<div class="container">
						<div class="header sidebar-header">${template-sidebar-title}</div>

						<div class="container-inner">
							${template-sidebar-contents}
						</div>
					</div>
				</td>
				<td align="left" valign="middle" class="container">
					<div class="container page-container">
						<div class="container-inner main-area">
							<div class="main-title-container">
								<h2 class="main-title">${template-main-title}</h2>
							</div>
							<br />
							<div class="error-container ${template-error-display}">
								<div class="error-panel">
									<div class="error-title">
										${template-error-title}
									</div>
									<div class="error-message">
										${template-error-message}
									</div>
								</div>
							</div>
							<br />
							<div>
								<form action="${template-form-destination}" method="post">
									<input type="hidden" name="sessionid" id="sessionid" value="${template-sessionid}" />
									${template-main-contents}
								</form>
							</div>
						</div>
					</div>
				</td>

			</tr>
		</table>
	</body>
</html>
